<resources xmlns:android="http://schemas.android.com/apk/res/android">				<!--******************* 标题栏样式 *******************-->    <!-- 标题栏.布局 -->    <style name="UA.TitleBar.Layout" parent="">        <item name="android:layout_width">fill_parent</item>        <item name="android:layout_height">@dimen/UA.TitleBar.Layout.Height</item>        <item name="android:background">@drawable/ua_shape_titlebar_bg</item>        <item name="android:gravity">center_vertical</item>    </style>    <!-- 标题栏 .标题文字 -->    <style name="UA.TitleBar.Title" parent="">        <item name="android:layout_width">wrap_content</item>        <item name="android:layout_height">wrap_content</item>        <item name="android:singleLine">true</item>        <item name="android:textSize">@dimen/UA.TitleBar.Title.TextSize</item>        <item name="android:textColor">@color/UA.TitleBar.Title.TextColor</item>        <item name="android:clickable">true</item>        <item name="android:padding">0dip</item>        <item name="android:ellipsize">end</item>        <item name="android:onClick">onTitleBarClicked</item>        <item name="android:layout_centerInParent">true</item>    </style>    <!-- 标题栏.进度条 -->    <style name="UA.TitleBar.Progress" parent="android:attr/progressBarStyle">        <item name="android:layout_width">@dimen/UA.TitleBar.Progress.Size</item>        <item name="android:layout_height">@dimen/UA.TitleBar.Progress.Size</item>        <item name="android:indeterminateDrawable">@drawable/ua_anim_progress_circle</item>        <item name="android:indeterminateDuration">700</item>        <item name="android:indeterminateBehavior">repeat</item>    </style>    <!-- 标题栏.图标按钮 -->    <style name="UA.TitleBar.ImageButton" parent="">        <item name="android:layout_width">@dimen/UA.TitleBar.ImageButton.Width</item>        <item name="android:layout_height">@dimen/UA.TitleBar.ImageButton.Height</item>        <item name="android:layout_gravity">center_vertical</item>        <item name="android:clickable">true</item>        <item name="android:focusable">true</item>        <item name="android:padding">@dimen/UA.TitleBar.ImageButton.Padding</item>        <item name="android:scaleType">fitXY</item>        <item name="android:onClick">onTitleBarClicked</item>        <item name="android:background">@drawable/ua_selector_titlebar_btn</item>    </style>        <!-- 标题栏.图标按钮.返回 -->    <style name="UA.TitleBar.ImageButton.Back" parent="UA.TitleBar.ImageButton">        <item name="android:src">@drawable/ua_ic_titlebar_back</item>        <item name="android:layout_centerVertical">true</item>        <item name="android:layout_alignParentLeft">true</item>    </style>        <!-- 标题栏.图标按钮.菜单 -->    <style name="UA.TitleBar.ImageButton.Menu" parent="UA.TitleBar.ImageButton">        <item name="android:src">@drawable/ua_ic_titlebar_menu</item>    </style>		<!--******************* 分隔线样式 *******************-->	<!-- 分隔线.实线.横向 -->    <style name="UA.Divider.Solid.Horizontal"  parent="">        <item name="android:layout_width">fill_parent</item>        <item name="android:layout_height">@dimen/UA.Divider.Solid.Horizontal.Height</item>        <item name="android:background">@color/UA.Divider</item>    </style>		<!--******************* Button：按钮样式 *******************-->    <style name="UA.Button.Theme" parent="">        <item name="android:layout_width">wrap_content</item>        <item name="android:layout_height">@dimen/UA.Button.Flat.Height</item>        <item name="android:paddingLeft">@dimen/UA.Button.Flat.PaddingLeft</item>        <item name="android:paddingRight">@dimen/UA.Button.Flat.PaddingRight</item>        <item name="android:paddingTop">@dimen/UA.Button.Flat.PaddingTop</item>        <item name="android:paddingBottom">@dimen/UA.Button.Flat.PaddingBottom</item>        <item name="android:minWidth">@dimen/UA.Button.Flat.MinWidth</item>        <item name="android:minHeight">@dimen/UA.Button.Flat.MinHeight</item>        <item name="android:gravity">center_vertical|center_horizontal</item>        <item name="android:textSize">@dimen/UA.Button.Flat.TextSize</item>        <item name="android:textColor">@drawable/ua_selector_btn_flat_text</item>        <item name="android:background">@drawable/ua_selector_btn_flat_theme</item>    </style>		<!--******************* EditTExt：编辑框样式 *******************-->    <style name="UA.EditText.Layout" parent="">        <item name="android:layout_width">fill_parent</item>        <item name="android:layout_height">52dp</item>        <item name="android:paddingLeft">15dp</item>        <item name="android:paddingRight">15dp</item>        <item name="android:gravity">center_vertical</item>        <item name="android:orientation">horizontal</item>        <item name="android:background">#FFFFFF</item>    </style>        <style name="UA.EditText.DrawableLeft" parent="">        <item name="android:layout_width">wrap_content</item>        <item name="android:layout_height">wrap_content</item>        <item name="android:layout_marginRight">15dp</item>        <item name="android:maxHeight">21dp</item>        <item name="android:maxWidth">17dp</item>    </style>        <style name="UA.EditText.DrawableRight" parent="">        <item name="android:layout_width">30dp</item>        <item name="android:layout_height">30dp</item>        <item name="android:maxHeight">16dp</item>        <item name="android:maxWidth">16dp</item>        <item name="android:background">@null</item>    </style>        <style name="UA.EditText.Theme" parent="">        <item name="android:layout_width">wrap_content</item>        <item name="android:layout_height">wrap_content</item>        <item name="android:textSize">18sp</item>        <item name="android:textColor">#333333</item>        <item name="android:textColorHint">#d3d6d5</item>        <item name="android:background">@null</item>    </style>    	<!--******************* TextView：文本框样式 *******************-->    <style name="UA.TextView.Theme" parent="">        <item name="android:layout_width">wrap_content</item>        <item name="android:layout_height">wrap_content</item>        <item name="android:textSize">16sp</item>        <item name="android:textColor">@color/UA.ThemeColor</item>    </style>        <style name="UA.TextView.Normal" parent="">        <item name="android:layout_width">wrap_content</item>        <item name="android:layout_height">wrap_content</item>        <item name="android:textColor">@color/UA.TextView.Normal</item>        <item name="android:textSize">16sp</item>    </style>        <style name="UA.TextView.Normal.Small" parent="">        <item name="android:layout_width">wrap_content</item>        <item name="android:layout_height">wrap_content</item>        <item name="android:textColor">@color/UA.TextView.Normal</item>        <item name="android:textSize">13sp</item>    </style>        <style name="UA.TextView.LightGray" parent="">        <item name="android:layout_width">wrap_content</item>        <item name="android:textColor">@color/UA.TextView.LightGray</item>        <item name="android:layout_height">wrap_content</item>        <item name="android:textSize">16sp</item>    </style>    	<!-- 弹出提示对话框的样式 -->    <style name="UA.Common.Dialog" parent="android:style/Theme.Dialog">        <item name="android:windowIsTranslucent">true</item>        <item name="android:windowNoTitle">true</item>        <item name="android:windowIsFloating">true</item>    </style></resources>