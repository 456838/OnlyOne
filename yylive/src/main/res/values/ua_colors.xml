<!-- 这部分样式，主要用于控制标题栏、选项卡栏、底部栏等 --><resources>    <color name="UA.ThemeColor">#ff8b00</color><!-- 主色调 -->    <color name="UA.ThemeColor10">#ff8200</color><!-- 主色调.亮度-10% -->    <color name="UA.ThemeColor20">#ff7b00</color><!-- 主色调.亮度-20% -->        <color name="UA.Activity.Background">#f6f6f6</color><!-- 内容页面背景 -->        <!-- 标题栏.背景 -->    <color name="UA.TitleBar.Background">@color/UA.ThemeColor</color> <!-- 标题栏.背景色 -->    <color name="UA.TitleBar.Divider">#D0D0D0</color> <!-- 标题栏.分隔线 -->    <color name="UA.TitleBar.Title.TextColor">#FFFFFF</color> <!-- 标题栏.标题文字 -->    <color name="UA.TitleBar.Button.Normal">#00000000</color> <!-- 标题栏.图标按钮.默认 -->    <color name="UA.TitleBar.Button.Pressed">#10000000</color> <!-- 标题栏.图标按钮.被点击 -->    <color name="UA.TitleBar.Button.Text.Normal">#FFFFFF</color> <!-- 标题栏.文字按钮.默认 -->    <color name="UA.TitleBar.Button.Text.Pressed">#88ffffff</color> <!-- 标题栏.文字按钮.被点击 -->    <color name="UA.TitleBar.Button.Tab.Normal">@color/UA.ThemeColor</color> <!-- 标题栏.分页按钮.默认 -->    <color name="UA.TitleBar.Button.Tab.Activited">#FFFFFF</color> <!-- 标题栏.分页按钮.被点击 -->      <!-- 选项卡栏 -->      <!-- 分隔线 -->    <color name="UA.Divider">#d3d3d3</color>        <!-- 按钮.扁平化.字体.默认 -->    <color name="UA.Button.Flat.Text.Normal">#FFFFFFFF</color> <!-- 按钮.扁平化.字体.不可用 -->    <color name="UA.Button.Flat.Text.Disabled">#adadad</color> <!-- 按钮.扁平化.字体.激活 -->    <color name="UA.Button.Flat.Text.Activited">#88FFFFFF</color> <!-- 按钮.扁平化.字体.默认.反色 -->    <color name="UA.Button.Flat.Theme.Nomal">@color/UA.ThemeColor</color>    <color name="UA.Button.Flat.Theme.Pressed">@color/UA.ThemeColor20</color>    <color name="UA.Button.Flat.Theme.Disabled">#d8d8d8</color>    <color name="UA.Button.Flat.Theme.Border">@color/UA.ThemeColor</color>    <color name="UA.Button.Flat.Theme.Border.Disabled">#d8d8d8</color>    <!-- 按钮.扁平化.字体.默认 -->    <color name="UA.TextView.LightGray">#c3c3c3</color>    <color name="UA.TextView.Normal">#7d7d7d</color>    <color name="UA.TextView.StrikingColor">@color/UA.ThemeColor</color>        <!-- 对话框.按钮.颜色-->    <color name="UA.Dialog.Btn.Text.Normal">#2192d9</color>    <color name="UA.Dialog.Btn.Text.Disabled">#adadad</color>    <color name="UA.Dialog.Btn.Text.Activited">#992192d9</color>   </resources>